@import '../../../common'

.corner
  position absolute
  width auto
  top -190px
  left -165px
  z-index -1
  +media-lt(lg)
    display none

.section
  background white
  color teal

.container
  display flex
  +media-lt(md)
    flex-direction column

.content
  flex 1

.list
  padding 30px
  +media-lt(md)
    list-style-type none
    padding 0

.item
  padding 20px 0
  +media-lt(md)
    padding 0
    margin 15px 0

.background
  flex 1
